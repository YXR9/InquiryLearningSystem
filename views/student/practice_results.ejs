        <div class="continaer-fluid" id="groupPracticeResults">
            <div class="row">
                <div class="col-3">
                    <div id="practiceList">
                        <div class="card" id="directiveObservationList">
                            <p class="card-header practiceType">
                                <a class="d-block practiceTypeBtn" data-toggle="collapse" href="#directiveObservationMission" aria-expanded="true" aria-controls="directiveObservationMission">
                                    <img class="pr-2" src="/images/icons/visibility-white.svg" alt="直接觀察">直接觀察任務<i class="fa fa-chevron-down float-right chevron"></i>
                                </a>
                            </p>
                            <div id="directiveObservationMission" class="collapse show"></div>
                        </div>
                        <div class="card" id="operationalObservationList">
                            <p class="card-header practiceType">
                                <a class="d-block practiceTypeBtn" data-toggle="collapse" href="#operationalObservationMission" aria-expanded="true" aria-controls="operationalObservationMission">
                                    <img class="pr-2" src="/images/icons/operational-observation-white.svg" alt="操作觀察">操作觀察任務<i class="fa fa-chevron-down float-right chevron"></i>
                                </a>
                            </p>
                            <div id="operationalObservationMission" class="collapse show"></div>
                        </div>
                        <div class="card" id="experimentList">
                            <p class="card-header practiceType">
                                <a class="d-block practiceTypeBtn" data-toggle="collapse" href="#experimentMission" aria-expanded="true" aria-controls="experimentMission">
                                    <img class="pr-2" src="/images/icons/experiment-white.svg" alt="實驗">實驗任務<i class="fa fa-chevron-down float-right chevron"></i>
                                </a>
                            </p>
                            <div id="experimentMission" class="collapse show"></div>
                        </div>
                    </div>                    
                </div>
                <div class="col-9">  
                    <div id="practiceCardList"></div>
                    <!-- <div class="row">
                        <div class="card practiceCard">
                            <div class="card-header practiceTitle"></div>
                            <div class="card-body practiceInfo"></div>
                            <div class="card-footer practiceRecord"></div>
                        </div>
                    </div> -->
                        
                </div>
            </div>           
        </div>
        <div id="activityId" style="display: none;"><%= activityId %></div>
        <div id="groupId" style="display: none;"><%= groupId %></div>
        <div id="memberId" style="display: none;"><%= memberId %></div>
        <div id="memberName" style="display: none;"><%= memberName %></div>
        <div id="directiveObservationListData" style="display: none;"><%= directiveObservationList %></div>
        <div id="operationalObservationListData" style="display: none;"><%= operationalObservationList %></div>
        <div id="experimentListData" style="display: none;"><%= experimentList %></div>
        <div id="directiveObservationData" style="display: none;"><%= directiveObservationData %></div>
        <div id="operationalObservationData" style="display: none;"><%= operationalObservationData %></div>
        <div id="experimentData" style="display: none;"><%= experimentData %></div>
        <script src="/socket.io/socket.io.js"></script>
        <script type="text/javascript" src="/javascripts/checkSession.js"></script>
        <script type="text/javascript" src="/javascripts/receive_broadcast.js"></script>
        <script type="text/javascript" src="/javascripts/join_socket_room.js"></script>
        <script src="/javascripts/group_practice_results.js"></script>
    </body>
